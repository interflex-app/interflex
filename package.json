{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "web:dev": "turbo run dev --filter=./apps/web",
    "web:build": "turbo run build --filter=./apps/web",
    "docs:dev": "turbo run dev --filter=./apps/docs",
    "docs:build": "turbo run build --filter=./apps/docs",
    "test:dev": "turbo run dev --filter=./apps/test",
    "test:build": "turbo run build --filter=./apps/test",
    "cli:run": "turbo run build --filter=./packages/interflex-cli && node ./packages/interflex-cli/dist/index.js",
    "cli:pub": "turbo run pub --filter=./packages/interflex-cli",
    "next:pub": "turbo run pub --filter=./packages/interflex-next",
    "pkg:sync": "turbo run build --filter=./packages/* && pnpm install",
    "cli:test": "pnpm pkg:sync && turbo run build --filter=./packages/interflex-cli && cd apps/test && node ../../packages/interflex-cli/dist/index.js"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-main": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "interflex"
}
